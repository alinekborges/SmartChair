#JSGF V1.0;

grammar digits;

<digit> = oh    |
          zero  |
          one   |
          two   |
          three |
          four  |
          five  |
          six   |
          seven |
          eight |
          nine  |
          front |
          back  |
          stop  |
          left  |
          right;

public <digits> = <digit>+;
